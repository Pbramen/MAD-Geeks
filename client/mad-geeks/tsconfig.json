{
    "compilerOptions": {
        "resolveJsonModule": true,
        "outDir": "./built",
        "allowJs": true,
        "jsx": "react",
        "target": "es6",
        "allowUnreachableCode": true,
        "allowUnusedLabels": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "module": "CommonJS",
        "baseUrl": "./src",
        "paths": {
            "*" : ["*",]
        }

    },
    "include": ["./src/**/*"],
    
}